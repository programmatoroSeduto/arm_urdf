<?xml version="1.0" ?>
<robot name="name" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:macro name="inertia" params="m">
	<inertia>
		<mass value="${m}"/>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
	</inertia>
</xacro:macro>
	
</robot>
