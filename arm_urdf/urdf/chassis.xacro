<?xml version="1.0" ?>
<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!-- chassis and front caster wheel --> 
<link name="base_link">
	
	<pose>0 0 0.1 0 0 0</pose>
    
	<inertial>
		<mass 
			value="5" />
		
		<origin 
			rpy="0 0 0" 
			xyz="0 0 0"/>
		
		<inertia 
			ixx="0.0395416666667" iyy="0.106208333333" izz="0.106208333333" 
			ixy="0" ixz="0" iyz="0" />
	</inertial>
    
    
    <!-- collision and visual chassis --> 
	<collision name="collision_chassis">
		<geometry>
			<box size="0.5 0.3 0.07"/>
		</geometry>
	</collision>
    
    <visual>
		<origin rpy="0 0 0" xyz="0 0 0"/>
		<geometry>
			<box size="0.5 0.3 0.07"/>
		</geometry>
		<material name="blue"/>
	</visual>
	<!-- collision and visual chassis -->
    
    
    <!-- collision, physics and visual of the front caster wheel -->
	<collision name="caster_front_collision">
		<origin 
			rpy=" 0 0 0" 
			xyz="0.35 0 -0.05"/>
		<geometry>
			<sphere radius="0.05"/>
		</geometry>
		<surface>
			<friction>
				<ode>
					<mu>0</mu>
					<mu2>0</mu2>
					<slip1>1.0</slip1>
					<slip2>1.0</slip2>
				</ode>
			</friction>
		</surface>
	</collision>
	
	<visual name="caster_front_visual">
		<origin 
			rpy=" 0 0 0" 
			xyz="0.2 0 -0.05"/>
		<geometry>
			<sphere radius="0.05"/>
		</geometry>
	</visual>
	<!-- collision, physics and visual of the front caster wheel -->
	
</link>
<!-- chassis and front caster wheel --> 



<!-- place for laser sensors -->
<joint name="joint_laser" type="fixed">
	<parent link="base_link"/>
	<child link="laser"/>
	<origin xyz="0.15 0 0.05" rpy="0 0 0"/>
</joint>

<link name="laser">
	<inertial>
		<origin xyz="0 0 0" rpy="0 0 0" />
		<mass value="1" />
		<xacro:cylinder_inertia mass="1" r="0.05" l="0.1" />
	</inertial>

	<visual>
		<origin xyz="0 0 0" rpy="0 0 0" />
		<geometry>
			<cylinder radius="0.05" length="0.1"/>
		</geometry>
		<material name="white" />
	</visual>

	<collision>
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<geometry>
			<cylinder radius="0.05" length="0.1"/>
		</geometry>
	</collision>
</link>
<!-- place for laser sensors -->



<!-- Wheels -->
<!--
<xacro:link_wheel name="link_right_wheel" />
<xacro:joint_wheel name="joint_right_wheel"  
	child="link_right_wheel" parent="base_link" xx="-0.05" yy="0.15" zz="0"  />
	
<xacro:link_wheel name="link_left_wheel" />
<xacro:joint_wheel name="joint_left_wheel" 
	child="link_left_wheel" parent="base_link" xx="-0.05" yy="-0.15" zz="0"  />
-->
<link name="link_right_wheel">
	<inertial>
		<mass value="0.2"/>
		<origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
		<inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
	</inertial>
	<collision name="link_right_wheel_collision">
		<origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
		<geometry>
			<cylinder length="0.04" radius="0.1"/>
		</geometry>
	</collision>
	<visual name="link_right_wheel_visual">
		<origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
		<geometry>
			<cylinder length="0.04" radius="0.1"/>
		</geometry>
		<material name="red"/>
	</visual>
</link>
<link name="link_left_wheel">
	<inertial>
		<mass value="0.2"/>
		<origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
		<inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
	</inertial>
	<collision name="link_left_wheel_collision">
		<origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
		<geometry>
			<cylinder length="0.04" radius="0.1"/>
		</geometry>
	</collision>
	<visual name="link_left_wheel_visual">
		<origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
		<geometry>
			<cylinder length="0.04" radius="0.1"/>
		</geometry>
		<material name="red"/>
	</visual>
</link>
<joint name="joint_right_wheel" type="continuous">
	<origin rpy="0 0 0" xyz="-0.05 0.15 0"/>
	<child link="link_right_wheel"/>
	<parent link="base_link"/>
	<axis rpy="0 0 0" xyz="0 1 0"/>
	<limit effort="10000" velocity="1000"/>
	<joint_properties damping="1.0" friction="1.0"/>
</joint>
<joint name="joint_left_wheel" type="continuous">
	<origin rpy="0 0 0" xyz="-0.05 -0.15 0"/>
	<child link="link_left_wheel"/>
	<parent link="base_link"/>
	<axis rpy="0 0 0" xyz="0 1 0"/>
	<limit effort="10000" velocity="1000"/>
	<joint_properties damping="1.0" friction="1.0"/>
</joint>
<!-- Wheels -->

</robot>
